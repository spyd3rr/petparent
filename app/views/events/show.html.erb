<!--<p id="alert alert-success"><%#= notice %></p>-->

<div class="row">
  <div class="col-md-6">
<p>
  <b>Address:</b>
  <%= @event.address %>
</p>

<p>
  <b>Address2:</b>
  <%= @event.address2 %>
</p>

<p>
  <b>City:</b>
  <%= @event.city %>
</p>

    <p>
      <b>Coordinate:</b>
      <%= @event.coordinate.latitude if @event.coordinate %>,<%= @event.coordinate.longitude unless @event.coordinate.nil? %>
    </p>

<p>
  <b>Description:</b>
  <%= @event.description %>
</p>

<p>
  <b>Enddate:</b>
  <%= @event.endDate %>
</p>


<p>
  <b>Name:</b>
  <%= @event.name  %>
</p>

<p>
  <b>Price:</b>
  <%= @event.price %>
</p>

<p>
  <b>Reportflag:</b>
  <%= @event.reportFlag %>
</p>

<p>
  <b>Startdate:</b>
  <%= @event.startDate %>
</p>

<p>
  <b>State:</b>
  <%= @event.state %>
</p>

<p>
  <b>Tags:</b>
  <%= @event.event_tags %>
</p>

    <p>
      <b>Thumbnail:</b>
      <%= image_tag @event.thumbnail %>
    </p>

<p>
  <b>User:</b>
  <%= @event.user %>
</p>

<p>
  <b>Venue:</b>
  <%= @event.venue.name if @event.venue %>
</p>

<p>
  <b>Zip:</b>
  <%= @event.zip %>
</p>
</div>

  <div class="col-md-6">
    <p>
      <%= image_tag @event.image %>
    </p>
  </div>

  </div>



<div class="row">
  <br/>
  <div class="col-xs-1"><p><%= button_to 'Edit', edit_event_path(@event), :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-2"><p><%= button_to 'Back', events_path, :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-10"></div>
</div>
