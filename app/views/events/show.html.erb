<!--<p id="alert alert-success"><%#= notice %></p>-->

<div class="row">
  <div class="col-md-6">
<p>
  <b>Address:</b>
  <%= @event.address %>
</p>

<p>
  <b>Address2:</b>
  <%= @event.address2 %>
</p>

<!--<p>
  <b>City:</b>
  <%#= @event.city %>
</p>-->

    <p>
      <b>Coordinate:</b>
      <%= @event.coordinate.latitude if @event.coordinate %>,<%= @event.coordinate.longitude unless @event.coordinate.nil? %>
    </p>

<p>
  <b>Description:</b>
  <%= @event.description %>
</p>

<p>
  <b>Enddate:</b>
  <% e=Parse::Query.new("Event").eq("objectId", @event.id).get.first %>
  <%= e["endDate"].to_time.localtime if e["startDate"] %>
</p>


<p>
  <b>Name:</b>
  <%= @event.name  %>
</p>

<p>
  <b>Price:</b>
  <%= @event.price %>
</p>

<p>
  <b>Reportflag:</b>
  <%= @event.reportFlag %>
</p>

<p>
  <b>Startdate:</b>
  <%= e["startDate"].to_time.localtime if e["startDate"] %>
  <%#= @event.startDate %>
</p>

<!--<p>
  <b>State:</b>
  <%#= @event.state %>
</p>-->

<p>
  <b>Tags:</b>
  <%= @event.event_tags %>
</p>

    <p>
      <b>Thumbnail:</b>
      <%= image_tag @event.thumbnail , :size => "50x50"%>
    </p>

<!--<p>
  <b>User:</b>
  <%#= @event.user %>
</p>-->

<p>
  <b>Venue:</b>
  <%= @event.venue.name if @event.venue %>
</p>
<!--
<p>
  <b>Zip:</b>
  <%#= @event.zip %>
</p>-->

    <p>
      <b>websiteUrl:</b>
      <%= @event.websiteUrl %>
    </p>

    <p>
      <b>ticketingUrl:</b>
      <%= @event.ticketingUrl %>
    </p>

</div>

  <div class="col-md-6">
    <p>
      <%= image_tag @event.image, :size => "50x50" %>
    </p>
  </div>

  </div>



<div class="row">
  <br/>
  <div class="col-xs-1"><p><%= button_to 'Edit', edit_event_path(@event), :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-2"><p><%= button_to 'Back', events_path, :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-3"><p><%= button_to 'Add Another Event', new_event_path, :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-10"></div>
</div>
