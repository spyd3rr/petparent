<% @events.each do |event| %>
  <tr>
    <td><%= event.address %></td>
    <!--<td><%#= event.address2 %></td>-->
    <!--<td><%#= event.city %></td>-->
    <!--<td><%#= event.coordinate %></td>-->
    <!--<td><%#= event.description %></td>-->
    <!--<td><%#= event.endDate %></td>-->
    <!--<td><%#= event.image %></td>-->
    <td><%= event.name %></td>
    <!--<td><%#= event.price %></td>-->
    <!--<td><%#= event.reportFlag %></td>-->
    <td><%= event.startDate %></td>
    <td><%= event.state %></td>
    <td><%= event.tags %></td>
    <!--<td><%#= event.thumbnail %></td>-->
    <!--<td><%#= event.user %></td>-->
    <td><%= event.venue.name unless event.venue.nil? %></td>
    <td><%= event.zip %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

