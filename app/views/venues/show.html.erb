<div class="row">
  <div class="col-md-6">
    <p>
      <b>Address:</b>
      <%= @venue.address %>
    </p>

    <p>
      <b>Address2:</b>
      <%= @venue.address2 %>
    </p>

    <p>
      <b>City:</b>
      <%= @venue.city %>
    </p>

    <p>
      <b>Coordinate:</b>
      <%= @venue.coordinate.latitude %>,<%= @venue.coordinate.longitude %>
    </p>

    <p>
      <b>Description:</b>
      <%= @venue.description %>
    </p>

    <p>
      <b>Favoriteusers:</b>
      <%= @venue.favoriteUsers %>
    </p>

    <p>
      <b>Name:</b>
      <%= @venue.name %>
    </p>

    <p>
      <b>Phone:</b>
      <%= @venue.phone %>
    </p>

    <p>
      <b>Photos:</b>
      <%= @venue.photos %>
    </p>

    <p>
      <b>Rating:</b>
      <%= @venue.rating %>
    </p>

    <p>
      <b>Reportflag:</b>
      <%= @venue.reportFlag %>
    </p>

    <p>
      <b>State:</b>
      <%= @venue.state %>
    </p>

    <p>
      <b>Tags:</b>
      <%= @venue.venue_tags %>
    </p>

    <p>
      <b>Thumbnail:</b>
      <%= image_tag @venue.thumbnail %>
    </p>

    <p>
      <b>Tips:</b>
      <%= @venue.tips %>
    </p>

    <p>
      <b>Zip:</b>
      <%= @venue.zip %>
    </p>

    <p>
      <b>websiteUrl:</b>
      <%= @venue.websiteUrl %>
    </p>


  </div>

  <div class="col-md-6">
    <p>
      <%= image_tag @venue.image %>
    </p>
  </div>

</div>


<div class="row">
  <br/>
  <div class="col-xs-1"><p><%= button_to 'Edit', edit_venue_path(@venue), :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-2"><p><%= button_to 'Back', venues_path, :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-3"><p><%= button_to 'Add Another Venue', new_venue_path, :class=> 'btn btn-primary', :method => :get  %></p></div>
  <div class="col-xs-10"></div>
</div>


